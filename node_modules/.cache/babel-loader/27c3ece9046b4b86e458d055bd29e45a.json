utton type=\"button\" class=\"btn ed_btn pull-center ed_orange\" @click=\"submitQuiz\">Submit</button>\r\n\r\n        </div>\r\n\r\n        <template v-if=\"quizResult == 'correct'\">\r\n                <div className=\"notification is-success mt-4\">\r\n                    Correct :-D\r\n\r\n                </div>\r\n\r\n        </template>\r\n\r\n        <template v-if=\"quizResult == 'incorrect'\">\r\n                <div className=\"notification is-danger mt-4\">\r\n                    wrong answer :-( Please try again !)\r\n\r\n                </div>\r\n\r\n        </template>\r\n\r\n\r\n    </div>\r\n\r\n</div>\r\n\r\n    \r\n</template>\r\n<script>\r\n\r\nexport default{\r\n        props: ['quiz'],\r\n        data(){\r\n            return {\r\n                selectedAnswer:'',\r\n                quizResult:null\r\n            }\r\n        },\r\n        methods:{\r\n            submitQuiz(){\r\n                this.quizResult = null\r\n                if (this.selectedAnswer) {\r\n                    if (this.selectedAnswer  === this.quiz.answer) {\r\n                        this.quizResult = 'correct'\r\n                    }else{\r\n                        this.quizResult = 'incorrect'\r\n                    }\r\n                }else{\r\n                    alert(\"Select answer first\")\r\n                }\r\n            },\r\n        }\r\n    }\r\n</script>\r\n"],"mappings":"AAmDA,eAAc;EACNA,KAAK,EAAE,CAAC,MAAM,CAAC;EACfC,IAAIA,CAAA,EAAE;IACF,OAAO;MACHC,cAAc,EAAC,EAAE;MACjBC,UAAU,EAAC;IACf;EACJ,CAAC;EACDC,OAAO,EAAC;IACJC,UAAUA,CAAA,EAAE;MACR,IAAI,CAACF,UAAS,GAAI,IAAG;MACrB,IAAI,IAAI,CAACD,cAAc,EAAE;QACrB,IAAI,IAAI,CAACA,cAAa,KAAO,IAAI,CAACI,IAAI,CAACC,MAAM,EAAE;UAC3C,IAAI,CAACJ,UAAS,GAAI,SAAQ;QAC9B,CAAC,MAAI;UACD,IAAI,CAACA,UAAS,GAAI,WAAU;QAChC;MACJ,CAAC,MAAI;QACDK,KAAK,CAAC,qBAAqB;MAC/B;IACJ;EACJ;AACJ"}]}