'',\r\n                quizResult:null\r\n            }\r\n        },\r\n        methods:{\r\n            submitQuiz(){\r\n                this.quizResult = null\r\n                if (this.selectedAnswer) {\r\n                    if (this.selectedAnswer  === this.quiz.answer) {\r\n                        this.quizResult = 'correct'\r\n                    }else{\r\n                        this.quizResult = 'incorrect'\r\n                    }\r\n                }else{\r\n                    alert(\"Select answer first\")\r\n                }\r\n            },\r\n        }\r\n    }\r\n</script>\r\n"]}]}